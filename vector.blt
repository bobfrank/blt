template<typename T>
class vector
{
    using range;
    init;
    init(range<T>& init_list); // like c++0x initializer list... not sure it'll be a range though.. not as fast as it could be
    push_back(const T& item);
    size => (int size);
    reserve(int size);
    resize(int size, const T& t=T() );
    get_range => (range<T>& iterator);
    operator[] (int offset) => (T& item);
}

T* vector::data;
int vector::size;
int vector::reserved;

vector::init
    : data(0)
    , size(0)
    , reserved(0)
{
}

vector::init(range& init_list)
    : data(0)
    , size(0)
    , reserved(0)
{
    while( !init_list.empty() ) { // need to figure out the best way..
        self.push_back(item=init_list.front());
        init_list.pop_front();
    }
}

vector::push_back

vector::get_range => (range<T> iterator)
{
    range<T> iterator(begin = &self.data[0],
                      end   = &self.data[self.size-1]);
    return(iterator=iterator);
}

